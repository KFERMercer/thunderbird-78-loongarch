### This file contains the entities needed for the 'edit' menu
### It's currently only used for the Browser Console and Developer Toolbox

editmenu-undo =
    .label = අවලංගු
    .accesskey = U

editmenu-redo =
    .label = යළි කරන්න
    .accesskey = R

editmenu-cut =
    .label = කපන්න
    .accesskey = t

editmenu-copy =
    .label = පිටපත්
    .accesskey = C

editmenu-paste =
    .label = අලවන්න
    .accesskey = P

editmenu-delete =
    .label = මකන්න
    .accesskey = D

editmenu-select-all =
    .label = සියල්ල තෝරන්න
    .accesskey = A
